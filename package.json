{
  "name": "manager-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --exit",
    "start": "pm2 start ./startup/ecosystem.config.js",
    "manager": "pm2 start ./startup/ecosystem_manager.config.js",
    "touchscreen": "pm2 start ./startup/ecosystem_touchscreen.config.js",
    "server": "pm2 start ./startup/ecosystem_server.config.js",
    "monitor": "pm2 monit",
    "log": "pm2 log",
    "status": "pm2 status",
    "dump": "pm2 flush",
    "kill": "pm2 kill",
    "api": "nodemon ./services/manager-api/index.js",
    "client": "nodemon ./services/manager-client/index.js",
    "http": "nodemon ./services/static-assets/http.js",
    "https": "nodemon ./services/static-assets/https.js",
    "static": "concurrently \"npm run http\" \"npm run https\"",
    "bulk": "nodemon ./services/manager-bulk/index.js",
    "auth": "nodemon ./services/manager-auth/index.js",
    "tax": "nodemon ./services/manager-tax/index.js",
    "invoice": "nodemon ./services/manager-invoice/index.js",
    "nodeapi": "node ./services/manager-api/index.js",
    "nodeclient": "node ./services/manager-client/index.js",
    "nodebulk": "node ./services/manager-bulk/index.js",
    "nodestatic": "node ./services/static-assets/http.js",
    "nodeauth": "node ./services/manager-auth/index.js",
    "nodetax": "node ./services/manager-tax/index.js",
    "nodeinvoice": "node ./services/manager-invoice/index.js",
    "dev": "concurrently \"npm run nodeapi\" \"npm run nodeclient\" \"npm run nodebulk\" \"npm run nodeauth\" \"npm run nodetax\" \"npm run nodeinvoice\" \"npm run nodestatic\"",
    "nodemon": "concurrently \"npm run api\" \"npm run client\" \"npm run bulk\" \"npm run auth\" \"npm run tax\" \"npm run invoice\" \"npm run static\"",
    "build-client": "cd services/manager-client/client && npm i && npm run build",
    "build-touchscreen": "cd services/touchscreen-client/client && npm i && npm run build",
    "build": "npm run build-client && npm run build-touchscreen"
  },
  "keywords": [],
  "author": "Matthew Lyons",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chokidar": "^3.4.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "express-validator": "^6.5.0",
    "fs-extra": "^9.0.1",
    "https": "^1.0.0",
    "jimp": "^0.10.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "moment": "^2.26.0",
    "mongodb-memory-server": "^6.9.2",
    "mongoose": "^5.10.9",
    "nodemailer": "^6.4.11",
    "pdf-to-printer": "^1.4.0",
    "puppeteer": "^2.1.1",
    "selfsigned": "^1.10.8",
    "sharp": "^0.25.4",
    "smartcrop": "^2.0.3",
    "smartcrop-sharp": "^2.0.3",
    "supertest": "^4.0.2",
    "underscore": "^1.10.2",
    "xml-js": "^1.6.11"
  }
}
