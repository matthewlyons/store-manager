        width: 780px;
        margin: auto;
    }

    .BorderLine {
        border-bottom: 1px solid black;
    }

    header {
        padding: 15px 0;
        height: 125px;
    }

    header .Logo {
        position: absolute;
        left: 15px;
        top: 30px;
    }

    header .Logo img {
        width: 203px;
    }

    header .Store-Address {
        position: absolute;
        left: 50%;
